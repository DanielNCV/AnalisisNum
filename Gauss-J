#Tarea  Gauss-Jordan
A=[1 -1 3 ; 1 1 1 ; 2 2 -1 ];
b=[13;11;17];
A=[A b];
X=[0,0,0];
n=3;
disp(A)
disp("\n")
for i=1:n-1
  j=i+1;
  while (j<=n)
    k=i;
    f=A(j,i)/A(i,i);
    while (k <= n+1)
      A(j,k)= A(j,k)- A(i,k)*f;
      k=k+1;
    endwhile
    j=j+1;
  endwhile
end

disp(A)
disp("\n")

for i=n:-1:2
  j=i-1;
  while (j>=1)
    k=i;
    f=A(j,i)/A(i,i);
    while (k <= n+1)
      A(j,k)= A(j,k)- A(i,k)*f;
      k=k+1;
    endwhile
    j=j-1;
  endwhile
end

disp(A)
disp("\n")
